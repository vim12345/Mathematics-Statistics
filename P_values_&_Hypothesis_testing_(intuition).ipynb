        "import numpy as np\n",
        "from scipy import stats\n",
        "\n",
        "a = np.random.normal(0, 1, size=30)\n",
        "b = np.random.normal(0.5, 1, size=30)\n",
        "tstat, pval = stats.ttest_ind(a, b)\n",
        "print(\"t-statistic:\", tstat, \"p-value:\", pval)\n"
      ]
    }
